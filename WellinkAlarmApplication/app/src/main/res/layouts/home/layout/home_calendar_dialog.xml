<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/calendar_dialog_background"
    android:padding="25dp"
    android:backgroundTint="@color/white">

    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="40dp">

        <TextView
            android:id="@+id/homeCalendarDialog_textView_dayOfMonth"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="O일"
            android:textSize="31sp"
            android:textColor="@color/toss_black_700"
            android:fontFamily="@font/ps_regular"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="15dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:weightSum="1">
        <TextView
            android:textColor="@color/toss_black_700"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:text="아침"
            android:gravity="center_vertical"
            android:fontFamily="@font/ps_regular"
            android:textSize="18sp"/>
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.03"/>
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.82"
            android:orientation="vertical"
            android:weightSum="1">
            <TextView
                android:id="@+id/homeCalendarDialog_textView_notTaken_morning"
                android:gravity="bottom"
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="0.5"
                android:text="미복용  >"
                android:textSize="18sp"
                android:textColor="@color/toss_black_150"
                android:fontFamily="@font/ps_regular"/>
            <TextView
                android:id="@+id/homeCalendarDialog_textView_taken_morning"
                android:gravity="top"
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="0.5"
                android:text="복용  >"
                android:textSize="18sp"
                android:textColor="@color/toss_blue_200"
                android:fontFamily="@font/ps_bold"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:weightSum="1">
        <TextView
            android:textColor="@color/toss_black_700"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:text="점심"
            android:gravity="center_vertical"
            android:fontFamily="@font/ps_regular"
            android:textSize="18sp"/>
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.03"/>

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.82"
            android:orientation="vertical"
            android:weightSum="1">
            <TextView
                android:id="@+id/homeCalendarDialog_textView_notTaken_afternoon"
                android:gravity="bottom"
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="0.5"
                android:text="미복용  >"
                android:textSize="18sp"
                android:textColor="@color/toss_black_150"
                android:fontFamily="@font/ps_regular"/>
            <TextView
                android:id="@+id/homeCalendarDialog_textView_taken_afternoon"
                android:gravity="top"
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="0.5"
                android:text="복용  >"
                android:textSize="18sp"
                android:textColor="@color/toss_blue_200"
                android:fontFamily="@font/ps_bold"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:weightSum="1">
        <TextView
            android:textColor="@color/toss_black_700"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:text="저녁"
            android:gravity="center_vertical"
            android:fontFamily="@font/ps_regular"
            android:textSize="18sp"/>
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.03"/>

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.82"
            android:orientation="vertical"
            android:weightSum="1">
            <TextView
                android:id="@+id/homeCalendarDialog_textView_notTaken_evening"
                android:gravity="bottom"
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="0.5"
                android:text="미복용  >"
                android:textSize="18sp"
                android:textColor="@color/toss_black_150"
                android:fontFamily="@font/ps_regular"/>
            <TextView
                android:id="@+id/homeCalendarDialog_textView_taken_evening"
                android:gravity="top"
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="0.5"
                android:text="복용  >"
                android:textSize="18sp"
                android:textColor="@color/toss_blue_200"
                android:fontFamily="@font/ps_bold"/>
        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="20dp"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:orientation="horizontal"
        android:weightSum="1">
        <TextView
            android:textColor="@color/toss_black_700"
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.15"
            android:text="취침"
            android:gravity="center_vertical"
            android:fontFamily="@font/ps_regular"
            android:textSize="18sp"/>
        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.03"/>

        <LinearLayout
            android:layout_width="0dip"
            android:layout_height="match_parent"
            android:layout_weight="0.82"
            android:orientation="vertical"
            android:weightSum="1">
            <TextView
                android:id="@+id/homeCalendarDialog_textView_notTaken_night"
                android:gravity="bottom"
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="0.5"
                android:text="미복용  >"
                android:textSize="18sp"
                android:textColor="@color/toss_black_150"
                android:fontFamily="@font/ps_regular"/>
            <TextView
                android:id="@+id/homeCalendarDialog_textView_taken_night"
                android:gravity="top"
                android:layout_width="match_parent"
                android:layout_height="0dip"
                android:layout_weight="0.5"
                android:text="복용  >"
                android:textSize="18sp"
                android:textColor="@color/toss_blue_200"
                android:fontFamily="@font/ps_bold"/>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>