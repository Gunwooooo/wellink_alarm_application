<?xml version="1.0" encoding="utf-8"?>

<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="95dp"
    android:orientation="vertical"
    android:id="@+id/homeCalendarItem_view"
    android:clickable="true"
    android:focusable="true">

    <TextView
        android:id="@+id/homeCalendarItem_textView"
        android:background="@drawable/calendar_item_border"
        android:padding="9dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test"
        android:textSize="16dp"
        android:gravity="left"
        android:fontFamily="@font/ps_regular"
        android:textColor="@color/toss_black_700"/>
    <ImageView
        android:id="@+id/homeCalendarItem_imageView"
        android:layout_gravity="center"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:src="@drawable/check"/>

</LinearLayout>