<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.WellinkAlarmApplication" parent="Theme.AppCompat.Light">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/toss_blue_200</item>
        <item name="colorPrimaryVariant">@color/toss_blue_700</item>
        <item name="colorOnPrimary">#FFFFFF</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorOnPrimary</item>
        <item name="android:windowLightStatusBar">true</item>
        <!-- Customize your theme here. -->
        <!-- progress color -->
        <item name="colorAccent">@color/toss_blue_200</item>
    </style>
</resources>